{
  "extends": "airbnb",
  "rules": {
    "react/prop-types": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/label-has-for": "off",
    "no-restricted-syntax": "off",
    "react/no-did-mount-set-state": "off",
    "consistent-return": "off",
    "no-shadow": "off"
  },
  "env": {
    "browser": true
  }
}